#include <iostream>
#include <stack> 
using namespace std;
void countsort(int arr[],int n){
    
    int a[10]={0};
    int temt[n]={0};
    for(int i=0;i<n ;i++){
        a[arr[i]]++;
       // cout<<a[arr[i]]<<" "<<arr[i]<<endl;
    }
     for(int i=1;i<10;i++){
         a[i]=a[i]+a[i-1];
     }
     /*for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }*/
    cout<<endl;
     int p=n-1 ;
     while(p>=0){
         temt[--a[arr[p]]]=arr[p];
          p--;
     }
     for(int i=0;i<n;i++){
        cout<<temt[i]<<" ";
    }
     
}

int main() {
	// your code goes her
	int arr[]= {2,4,4,2,5,6,3};
	countsort(arr,7);
	
	return 0;
}
